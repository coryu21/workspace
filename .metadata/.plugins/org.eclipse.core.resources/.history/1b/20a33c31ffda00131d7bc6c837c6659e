public class Consumer extends Thread {
	Counter c;
	int num;
	public Consumer(Counter c, int n) {
		this.c = c;
	}

	public void run() {
		for (int i = 0; i < 15; i++) {
			c.decrement();
			System.out.println(getName() + " decrese");
			try {
				Thread.sleep(99);
			} catch (InterruptedException e) {
				System.out.println(c.getCount());
			}
		}
	}

}
