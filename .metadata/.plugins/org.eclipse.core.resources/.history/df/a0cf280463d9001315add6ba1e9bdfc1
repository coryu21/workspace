		this.list = list;
	}
	
	public void run(){
		init();
	}
	
	public void init(){
		Iterator it = list.iterator();
		int count = 0;
		String fileName;
		File temp;
		while(it.hasNext()){
			fileName = ((File)it.next()).getPath();
			System.out.println(fileName);
			FILE
			//date[count] = temp.lastModified();
			//System.out.println(date[count]);
			count++;
		}
	}

}
