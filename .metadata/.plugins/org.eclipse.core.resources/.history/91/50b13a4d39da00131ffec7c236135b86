public class Boyer {
	int skip[];

	public void initSkip(String pattern) {
		skip = new int[127];
		for (int i = 0; i < skip.length; i++) {
			skip[i] = pattern.length();
		}
		for (int i = 0; i < pattern.length(); i++) {
			skip[pattern.charAt(i)] = pattern.length() - i - 1;
		}
		
		System.out.println("패턴 : " + pattern + "\tskip[]");
	}
}
